<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE builder PUBLIC "-//MMBase//DTD builder config 1.1//EN" "http://www.mmbase.org/dtd/builder_1_1.dtd">
<builder name="editwizards" maintainer="mmbase.org" version="0" extends="object">
  <classfile></classfile>
  <searchage>9999</searchage>
  <names>
    <singular xml:lang="nl">Editwizard</singular>
    <singular xml:lang="fr">Editwizard</singular>
    <singular xml:lang="en">Editwizard</singular>
    <plural xml:lang="nl">Editwizards</plural>
    <plural xml:lang="fr">Editwizards</plural>
    <plural xml:lang="en">Editwizard</plural>
  </names>
  <fieldlist>
    <field>
      <gui>
        <guiname xml:lang="nl">Titel</guiname>
        <guiname xml:lang="en">Title</guiname>
        <guitype>string</guitype>
      </gui>
      <editor>
        <positions>
          <list>1</list>
        </positions>
      </editor>
      <db>
        <name>title</name>
        <type state="persistent" size="255" notnull="true" key="false">STRING</type>
      </db>
    </field>

    <field>
      <gui>
        <guiname xml:lang="nl">Url</guiname>
        <guiname xml:lang="en">Url</guiname>
        <guitype>field</guitype>
      </gui>
      <editor>
        <positions>
          <list>2</list>
          <search>2</search>
        </positions>
      </editor>
      <db>
        <name>url</name>
        <type state="persistent" size="512" notnull="true" key="false">STRING</type>
      </db>
    </field>

    <field>
      <gui>
        <guiname xml:lang="nl">Omschrijving</guiname>
        <guiname xml:lang="en">Description</guiname>
        <guitype>field</guitype>
      </gui>
      <db>
        <name>description</name>
        <type state="persistent" size="1024" notnull="true" key="false">STRING</type>
      </db>
    </field>

  </fieldlist>
</builder>
